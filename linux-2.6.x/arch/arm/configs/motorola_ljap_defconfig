# CONFIG_MXC_PMIC_SC55112 is not set

#
# Advanced Power Management devices
#

#
# MXC Security Drivers
#
CONFIG_MXC_SECURITY_SCC=y
CONFIG_MXC_SECURITY_RTIC=y
# CONFIG_MXC_RTIC_TEST_DEBUG is not set
CONFIG_MXC_SECURITY_CORE=y

#
# SAHARA2 Security Hardware Support
#
CONFIG_MXC_SAHARA=y
CONFIG_MXC_SAHARA_USER_MODE=y
# CONFIG_MXC_SAHARA_POLL_MODE is not set

#
# SDMA
#
CONFIG_MXC_SDMA_API=y

#
# MXC MPEG4 Encoder Kernel module support
#

#
# APAL (Audio Driver) support
#
CONFIG_APAL=y

#
# Do not hardcode pgprot to PAGE_SHARED
#
CONFIG_MOT_FEAT_NO_PAGE_SHARED=y

#
# GCOV support
#
# CONFIG_GCOV_PROFILE is not set 
# CONFIG_GCOV_PROC is not set
# CONFIG_GCOV_ALL is not set
# CONFIG_GCOV_HAMMER is not set

CONFIG_MOT_FEAT_SDMA_DEBUG=y

#
# UMA Stage 3 Spec, DSCP mapping
#
CONFIG_MOT_FEAT_DSCP_UMA=y

#
# Set TCP MSS to solve compatibility issue with CMCC
#
CONFIG_MOT_FEAT_MSS_CMCC=y


# CONFIG_MOT_TURBO_INDICATOR is not set
# CONFIG_MOT_DSM_INDICATOR is not set

# CONFIG_MOT_FEAT_DISPLAY_EPSON is not set

# CONFIG_MOT_FEAT_32_BIT_DISPLAY is not set
