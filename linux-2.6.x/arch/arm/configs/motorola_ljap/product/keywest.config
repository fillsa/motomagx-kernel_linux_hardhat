#
# NOTICE: Please avoid adding new entries to this file. For more details on
# the kernel configuration procedure, please see ../README.
#

CONFIG_MACH_KEYWEST=y
CONFIG_MOT_WFN418=y
CONFIG_MOT_FEAT_IPU_MEM_SIZE=y

#
# Motorola GPIO API Options
#
CONFIG_MOT_FEAT_GPIO_API_SERIALIZER=y
CONFIG_MOT_FEAT_GPIO_API_USBHS=y

#
# MMC/SD Card support
#
CONFIG_MMC=m
CONFIG_MMC_BLOCK=m
CONFIG_MMC_MXC=m
CONFIG_MOT_FEAT_MMC_SDHC1=y
CONFIG_MOT_FEAT_MMC_SDHC2=y

#CONFIG_MOT_POWER_IC_ATLAS is not set #different LJ use different atlas methods
CONFIG_MOT_FEAT_POWER_IC_API=y #different LJ use different atlas methods

CONFIG_MXC_IPC_V2=y
CONFIG_APAL=m #LJ71=m other=y
CONFIG_SIM_SUPPORT=m #LJ71=m other=y

CONFIG_MOT_FEAT_INVERTED_SPI_DISPLAY=m
CONFIG_BLK_DEV_INITRD=y

CONFIG_MXC_SSI=m
CONFIG_MXC_DAM=m

CONFIG_MOT_WFN487=y
CONFIG_MOT_FEAT_MOTHWCFG_DEVICE_TREE=y

CONFIG_MOT_FEAT_LSM=n

CONFIG_SECURITY_NETWORK=y
CONFIG_SECURITY_CAPABILITIES=y

